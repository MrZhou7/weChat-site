/* pages/siteView/siteView.wxss */
.content {
    display: flex;
    flex-wrap: wrap;
    height: auto;
    padding: 10rpx 0;
    /* justify-content: center; */
    line-height: normal;
    margin: 0;
    position: static;
}
.list{
    width: 236rpx;
    margin:0 0 10rpx 10rpx;
    height: 300rpx;
    text-align: center;
    color:#fff;
    position: relative;
}
.list view{
  text-align: center;
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 50px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  height: 220rpx;
  line-height: 220rpx;
}
.weui-cell {
  border-bottom: 1rpx solid rgba(0,0,0,.1);
}