.box{
  display: inline-block;
  margin: 20rpx;
  font-size: 26rpx;
  border: 1px solid #eee;
  border-radius: 30rpx;
  padding: 10rpx 20rpx;
}
.active{
  color: red;
  border-color: red;
}
.confirmBtn{
  font-size: 26rpx;
  padding: 5rpx 10rpx;
}
.list{
  display: flex;
  padding-left: 16px;
}
