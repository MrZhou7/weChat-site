/* pages/cleaning/cleaning.wxss */
.content {
  display: flex;
  flex-wrap: wrap;
  height: auto;
  padding: 10rpx 0;
  /* justify-content: center; */
  line-height: normal;
  margin: 0;
  position: static;
}
.list{
  width: 344rpx;
  margin:0 0 10rpx 20rpx;
  height: 308rpx;
  text-align: center;
  color:#fff;
  position: relative;
  border-radius: 20rpx;
}
.wrap{
  margin: 68rpx auto 40rpx;
  width: 154rpx;
  height: 154rpx;
}
.wrap image{
  width: 100%;
}
.weui-badge {
  position: absolute;
  top: 10px;
  right: 10px;
}
.list view{
  text-align: center;
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 50px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  height: 220rpx;
  line-height: 220rpx;
}

.weui-cell {
  border-bottom: 1rpx solid rgba(0,0,0,.1);
}